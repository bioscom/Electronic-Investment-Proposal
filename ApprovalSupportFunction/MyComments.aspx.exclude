            </td>
            <td>
                <asp:Label id="Label6" runat="server" Text="Date Submitted:" Font-Bold="True"></asp:Label>
            </td>
            <td>
                <asp:Label id="dateSubmitLabel" runat="server"></asp:Label>
            </td>
        </tr>
        <tr>
            <td>
                <asp:Label id="Label29" runat="server" Text="Amount JV ($mln):" 
