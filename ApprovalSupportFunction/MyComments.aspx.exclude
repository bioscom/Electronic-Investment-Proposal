                <asp:Label id="initiatorLabel" runat="server"></asp:Label>
            </td>
            <td>
                &nbsp;</td>
            <td>
                &nbsp;</td>
        </tr>
        <tr>
            <td colspan="4">
            <center>
                    <asp:Button ID="backButton" runat="server" Text="Previous Page" 
                    onclick="backButton_Click" />
            </center>
            </td>
        </tr>
        <tr>
            <td colspan="4">
            <div style="background-color:White">
            
    <asp:GridView id="MyCommentsGridView" runat="server" AutoGenerateColumns="False" Width="100%">
    <Columns>
    
        <asp:TemplateField>
            <ItemTemplate>
               <%# Container.DataItemIndex + 1 %>
            </ItemTemplate>
        </asp:TemplateField>
   
        <asp:TemplateField HeaderText="Stand">
            <ItemTemplate>
                <asp:Label id="labelStand" runat="server" Text='<%# DataBinder.Eval(Container.DataItem, "STAND") %>'></asp:Label>
            </ItemTemplate>
        </asp:TemplateField>
                
        <asp:TemplateField HeaderText="Comment">
            <ItemTemplate>
                <asp:Label id="labelDateForwarded" runat="server" Text='<%# DataBinder.Eval(Container.DataItem, "COMMENTS") %>'></asp:Label>
            </ItemTemplate>
        </asp:TemplateField>
        
        <asp:TemplateField HeaderText="Date">
            <ItemTemplate>
                <asp:Label id="labelProjectNumber" runat="server" Text='<%# DataBinder.Eval(Container.DataItem, "DATE_COMMENT") %>'></asp:Label>
            </ItemTemplate>
        </asp:TemplateField>
        
    </Columns>
    </asp:GridView>
            
            </div>
                &nbsp;</td>
        </tr>
        <tr>
            <td colspan="4" class="cHeadTile">
