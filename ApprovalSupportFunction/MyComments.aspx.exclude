<%@ Page Language="C#" MasterPageFile="~/MasterPages/eIPMasterPage.master" AutoEventWireup="True" CodeFile="MyComments.aspx.cs" Inherits="ApprovalSupportFunction_MyComments" Title="Electronic Investment Proposal" %>

